{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "cSpell.enableFiletypes": ["mdx"],
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "files.readonlyInclude": {
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/logs/**": true,
    "**/static/html5/**": true
  },
  "cSpell.words": ["Guidewire"]
}
