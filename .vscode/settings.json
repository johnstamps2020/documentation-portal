{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "cSpell.enableFiletypes": ["mdx"],
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": ["-i"],
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": ["-i"]
    },
    "fish": {
      "path": "fish",
      "args": ["-i"]
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "files.readonlyInclude": {
    "**/node_modules/**": true,
    "**/logs/**": true
  },
  "cSpell.words": [
    "AMST",
    "Aspen",
    "Banff",
    "brightcove",
    "Businessowners",
    "clsx",
    "Cortina",
    "Creds",
    "ditavals",
    "Dobson",
    "doctools",
    "Elysian",
    "Flaine",
    "fontsource",
    "Garmisch",
    "Guidewire",
    "Hakuba",
    "IRSA",
    "ISBTF",
    "redoc",
    "reindex",
    "reindexing",
    "superbook"
  ],
  "cSpell.ignoreRegExpList": [
    "\"(id|docId|srcId)\": \"[a-zA-Z0-9]+\"",
    "\"(url|root|filter|targetFolder|gitUrl|zipFilename)\": \"[^\"]+\""
  ]
}
