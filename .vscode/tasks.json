{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "html5 DEV",
      "type": "npm",
      "script": "dev:html5",
      "isBackground": true,
      "problemMatcher": {
        "owner": "custom",
        "fileLocation": ["relative", "${workspaceFolder}"],
        "pattern": [
          {
            "regexp": "^([^\\\\s].*)\\\\((\\\\d+,\\\\d+)\\\\):\\\\s*(.*)$",
            "file": 1,
            "location": 2,
            "message": 3
          }
        ],
        "background": {
          "activeOnStart": true,
          "endsPattern": ".*Successfully compiled .*",
          "beginsPattern": ".*webpack.*compiled with .*"
        }
      }
    }
  ]
}
