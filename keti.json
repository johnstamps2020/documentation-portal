{
  "name": "NODE_Hawaii_Docs_Web",
  "authType": "OIDC",
  "authSettings": {
    "applicationType": "WEB",
    "grantTypes": [
      "CLIENT_CREDENTIALS",
      "IMPLICIT",
      "REFRESH_TOKEN",
      "AUTHORIZATION_CODE"
    ],
    "responseTypes": ["TOKEN", "CODE", "ID_TOKEN"],
    "clientAuthenticationType": "CLIENT_SECRET_BASIC",
    "tokenValidity": {
      "accessTokenLifetimeMinutes": 60,
      "refreshTokenLifetimeMinutes": 10080,
      "refreshTokenWindowMinutes": 1440
    },
    "redirectUris": [
      "http://localhost:8080/authorization-code/callback",
      "https://nodeoktacontainer-doctools.dev.ccs.guidewire.net/authorization-code/callback"
    ]
  },
  "ownershipType": "GROUP",
  "ownerPod": "doctools"
}
