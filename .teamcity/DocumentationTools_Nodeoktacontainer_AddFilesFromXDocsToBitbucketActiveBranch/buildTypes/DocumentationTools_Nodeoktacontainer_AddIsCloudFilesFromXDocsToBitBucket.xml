<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Add IS cloud files from XDocs to BitBucket</name>
  <description />
  <settings ref="DocumentationTools_Nodeoktacontainer_AddFilesFromXDocsToBitbucket">
    <parameters>
      <param name="EXPORT_PATH_IDS" value="/Content/doc/insuranceSuite/core/cloud/active/_superbook.ditamap /SysConfig/publishProfiles/processingProfiles/filterSets/IS-PC-Cloud-Draft.ditaval /SysConfig/publishProfiles/processingProfiles/filterSets/IS-BC-Cloud-Draft.ditaval /SysConfig/publishProfiles/processingProfiles/filterSets/IS-CC-Cloud-Draft.ditaval /SysConfig/publishProfiles/processingProfiles/filterSets/IS-PC-Cloud-Release.ditaval /SysConfig/publishProfiles/processingProfiles/filterSets/IS-BC-Cloud-Release.ditaval /SysConfig/publishProfiles/processingProfiles/filterSets/IS-CC-Cloud-Release.ditaval" spec="text validationMode='not_empty'" />
      <param name="SOURCES_ROOT" value="src_root" spec="text validationMode='not_empty'" />
      <param name="XDOCS_EXPORT_DIR" value="%system.teamcity.build.tempDir%/xdocs_export_dir" spec="text validationMode='not_empty'" />
    </parameters>
    <build-runners />
    <vcs-settings>
      <vcs-entry-ref root-id="DocumentationTools_Nodeoktacontainer_InsurancesuiteCloud">
        <checkout-rule rule="+:. =&gt; %SOURCES_ROOT%" />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

