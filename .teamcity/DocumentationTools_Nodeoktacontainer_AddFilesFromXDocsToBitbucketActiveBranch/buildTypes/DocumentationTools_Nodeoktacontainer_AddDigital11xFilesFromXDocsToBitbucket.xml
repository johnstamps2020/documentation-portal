<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Add Digital11x files from XDocs to Bitbucket</name>
  <description>Exports DITA files from XDocs and adds them to Bitbucket</description>
  <settings ref="DocumentationTools_Nodeoktacontainer_AddFilesFromXDocsToBitbucket">
    <parameters>
      <param name="EXPORT_PATH_IDS" value="/SysConfig/publishProfiles/processingProfiles/filterSets/Digital-ce-am.ditaval /SysConfig/publishProfiles/processingProfiles/filterSets/Digital-ce-cb.ditaval /SysConfig/publishProfiles/processingProfiles/filterSets/Digital-ce-qb.ditaval /SysConfig/publishProfiles/processingProfiles/filterSets/Digital-pe.ditaval /SysConfig/publishProfiles/processingProfiles/filterSets/Digital-pe-sf.ditaval /SysConfig/publishProfiles/processingProfiles/filterSets/Digital-sre.ditaval /SysConfig/publishProfiles/processingProfiles/filterSets/Digital-sre-sf.ditaval /SysConfig/publishProfiles/processingProfiles/filterSets/Digital-ve.ditaval /Content/doc/digital/11.x/active/DX_superbook.ditamap" spec="text validationMode='not_empty'" />
      <param name="SOURCES_ROOT" value="src_root" spec="text validationMode='not_empty'" />
      <param name="XDOCS_EXPORT_DIR" value="%system.teamcity.build.tempDir%/xdocs_export_dir" spec="text validationMode='not_empty'" />
    </parameters>
    <build-runners />
    <vcs-settings>
      <vcs-entry-ref root-id="DocumentationTools_Nodeoktacontainer_Digital11">
        <checkout-rule rule="+:. =&gt; %SOURCES_ROOT%" />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

