<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Deploy to Dev</name>
  <description />
  <settings>
    <inherits>
      <ref id="RootProjectId_BuildDockerImage" />
      <ref id="RootProjectId_Deploy" />
    </inherits>
    <options>
      <option name="cleanBuild" value="true" />
      <option name="maximumNumberOfBuilds" value="1" />
    </options>
    <parameters>
      <param name="env.DEPLOY_ENV" value="dev" />
      <param name="env.NAMESPACE" value="doctools" spec="text label='Namespace' display='prompt' validationMode='not_empty'" />
      <param name="env.TAG_VERSION" value="latest" />
    </parameters>
    <build-runners />
    <vcs-settings />
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="buildDependencyTrigger">
        <parameters>
          <param name="afterSuccessfulBuildOnly" value="true" />
          <param name="dependsOn" value="RootProjectId_Test" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <dependencies>
      <depend-on sourceBuildTypeId="RootProjectId_Checkmarx">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="RootProjectId_Test">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

