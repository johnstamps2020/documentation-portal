<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Build Guidewire for Salesforce Digital Gateway for Claims Self-managed 3.1.0 (dxedconpremstaging)</name>
  <description />
  <settings>
    <inherits>
      <ref id="DocumentationTools_DocumentationPortal_CrawlDocumentAndUpdateIndex" />
      <ref id="DocumentationTools_DocumentationPortal_PublishBrokenLinksReportToS3" />
    </inherits>
    <options>
      <option name="cleanBuild" value="true" />
      <option name="maximumNumberOfBuilds" value="1" />
    </options>
    <parameters>
      <param name="APP_BASE_URL" value="https://docs.dev.ccs.guidewire.net" spec="text validationMode='not_empty'" />
      <param name="CONFIG_FILE_URL" value="https://ditaot.internal.dev.ccs.guidewire.net/portal-config/config.json" spec="text validationMode='not_empty'" />
      <param name="DOC_ID" value="dxedconpremstaging" spec="text validationMode='not_empty'" />
      <param name="DOC_S3_URL" value="https://ditaot.internal.dev.ccs.guidewire.net" spec="text validationMode='not_empty'" />
      <param name="ELASTICSEARCH_URLS" value="https://docsearch-doctools.dev.ccs.guidewire.net" spec="text validationMode='not_empty'" />
      <param name="INDEX_NAME" value="gw-docs" spec="text validationMode='not_empty'" />
      <param name="TOOLS_ROOT" value="tools_root" spec="text validationMode='not_empty'" />
      <param name="env.DITAVAL_FILE" value="Digital-ed-c-OnPrem.ditaval" spec="text validationMode='not_empty'" />
      <param name="env.FORMAT" value="wh-pdf" spec="text validationMode='not_empty'" />
      <param name="env.INPUT_PATH" value="DX_superbook.ditamap" spec="text validationMode='not_empty'" />
      <param name="env.PDF_TRANSTYPE" value="pdf5_Guidewire" spec="text validationMode='regex'" />
      <param name="env.PLATFORM" value="Self-managed" spec="text validationMode='not_empty'" />
      <param name="env.PRODUCT" value="Guidewire for Salesforce Digital Gateway for Claims" spec="text validationMode='not_empty'" />
      <param name="env.PUBLISH_PATH" value="self-managed/dx/ed-c/310" spec="text validationMode='not_empty'" />
      <param name="env.S3_BUCKET_NAME" value="tenant-doctools-dev-builds" spec="text validationMode='not_empty'" />
      <param name="env.SOURCES_ROOT" value="src_root" spec="text validationMode='not_empty'" />
      <param name="env.SSH_PASSWORD" value="credentialsJSON:a547ee60-435e-47c3-901e-a1255a38dd3b" spec="password" />
      <param name="env.SSH_USER" value="ssh_user" spec="text validationMode='not_empty'" />
      <param name="env.VERSION" value="3.1.0" spec="text validationMode='not_empty'" />
    </parameters>
    <build-runners order="RUN_DITA_BUILD, UPLOAD_GENERATED_CONTENT, BUILD_CRAWLER_DOCKER_IMAGE, CRAWL_DOC, PUBLISH_BROKEN_LINK">
      <runner id="RUN_DITA_BUILD" name="Run DITA build" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[chmod -R 777 ./
%env.DITA_OT_DIR%/bin/dita --install

%env.DITA_OT_DIR%/bin/dita --input="%env.SOURCES_ROOT%/%env.INPUT_PATH%" --format=%env.FORMAT% --use-doc-portal-params=yes --gw-product="%env.PRODUCT%" --gw-platform="%env.PLATFORM%" --gw-version="%env.VERSION%" --filter="%env.SOURCES_ROOT%/Digital-ed-c-OnPrem.ditaval" --git.url=ssh://git@stash.guidewire.com/docsources/digital-11x.git --dita.ot.pdf.format="%env.PDF_TRANSTYPE%"]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="UPLOAD_GENERATED_CONTENT" name="Upload generated content to the S3 bucket" type="simpleRunner">
        <parameters>
          <param name="script.content" value="aws s3 sync ./out s3://%env.S3_BUCKET_NAME%/%env.PUBLISH_PATH% --delete" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="DocumentationTools_DitaOtPlugins">
        <checkout-rule rule="+:. =&gt; ./%env.DITA_OT_PLUGINS_DIR%" />
      </vcs-entry-ref>
      <vcs-entry-ref root-id="dxstaging">
        <checkout-rule rule="+:. =&gt; %env.SOURCES_ROOT%" />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements />
    <build-triggers />
    <build-extensions>
      <extension id="DockerSupport" type="DockerSupport">
        <parameters>
          <param name="login2registry" value="PROJECT_EXT_155" />
          <param name="loginCheckbox" value="on" />
        </parameters>
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

