<!DOCTYPE html>
<html lang="en">
  <% include parts/head %>
  <body>
    <% include parts/header %>
    <div class="docPanelContainer">
      <div class="container">
        <div class="row">
          <div class="col-sm-9">
            <div class="docPageTitle">
              <h1><%= product.name %></h1>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-sm-9">
            <div class="docsInProduct">
              <% docs.forEach(doc => { -%>
              <div class="docLink">
                <a href="/<%= doc.url %>"><%= doc.title %></a>
              </div>
              <% }); -%>
            </div>
          </div>
          <div class="col-sm-3">
            <% include parts/useful-links %>
          </div>
        </div>
      </div>
    </div>
    <% include parts/footer %>
  </body>
</html>
