<!DOCTYPE html>
<html lang="en">
  <% include parts/head %>
  <body>
    <% include parts/header %>
    <div class="docPanelContainer">
      <div class="container">
        <div class="row">
          <div class="col">
            <div class="docPageTitle docPageHeaderItems">
              <h1><%= title %></h1>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col">
            <div class="row">
              <% categories.forEach(category => { -%>
                <div class="col-sm-4 mt-3">
                  <div class="categoryCard">
                  <h2><%= category.label %></h2>
                    <% category.docs.forEach(categoryDoc => { -%>
                      <div class="docLink">
                          <a href="" target="_blank" rel="noopener noreferrer"><%= categoryDoc %></a>
                      </div>
                    <% }) -%>
                    <% category.groups.forEach(group => { -%>
                    <h3><%= group.label %></h3>
                      <% group.items.forEach(groupDoc => { -%>
                        <div class="docLink">
                          <a href="" target="_blank" rel="noopener noreferrer"><%= groupDoc %></a>
                        </div>
                      <% }) -%>
                    <% }) -%>
                  </div>
                </div>
              <% }) -%>
            </div>
          </div>
        </div>
      </div>
    </div>
    <% include parts/footer %>
  </body>
</html>
