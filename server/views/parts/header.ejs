<% if (typeof headerClass !== 'undefined') { %>
<header class="<%= headerClass %>">
    <% } else { %>
    <header>
        <% } %>
        <div id="headerLeft">
            <a href="/">
                <img
                        class="pageLogo"
                        src="/images/logo-gw-doc.svg"
                        alt="Guidewire Documentation"
                        role="img"
                        aria-label="Guidewire Documentation"
                />
            </a>
        </div>
        <% if (!hideSearchBox) { %>
            <div id="headerCenter">
                <form action="/search">
                    <div class="searchWrapper">
                        <input
                                name="q"
                                id="searchField"
                                class="searchField"
                                type="search"
                                placeholder="<%= localizationInfo.placeholder %>"
                                aria-label="<%= localizationInfo.placeholder %>"
                        />
                        <button type="submit" class="searchButton" aria-label="<%= localizationInfo.placeholder %>">
                            <%= localizationInfo.placeholder %>
                        </button>
                    </div>
                </form>
            </div>
        <% } %>
        <div id="headerRight">
            <% include links-menu %>
            <% if (!hideLoginButton) { %>
                <div id="loginContainer">
                    <a class="button invisible" id="loginButton" href="/gw-login">Log in</a>
                </div>
            <% } %>
        </div>
    </header>
</header>
